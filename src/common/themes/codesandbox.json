{
  "$schema": "vscode://schemas/color-theme",
  "isCodeSandbox": true,
  "type": "dark",
  "colors": {
    "editor.background": "#1C2022",
    "editor.foreground": "#E0E0E0",
    "editor.selectionBackground": "#ADD6FF26"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment"],
      "settings": {
        "foreground": "#5C6370",
        "fontStyle": "italic"
      }
    }
  ]
}
